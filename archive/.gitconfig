[user]
	name = Jack Tonkin
	email = jack@tonkin.org.uk
[core]
	excludesfile = /Users/jack/.gitignore_global
	pager = diff-so-fancy | less --tabs=4 -RFX
[color]
	ui = true
[push]
	default = simple
[color "diff"]
	meta = yellow bold
	commit = green bold
	frag = magenta bold
	old = red bold
	new = green bold
	whitespace = red reverse
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = "red bold 52"
	newNormal = "green bold"
	newHighlight = "green bold 22"
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
